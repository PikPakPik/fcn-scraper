{
  "name": "fcn-scraper",
  "version": "1.0.0",
  "description": "Scraper automatis√© pour le calendrier des matchs du FC Nantes",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "scrape": "tsx src/index.ts",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["fc-nantes", "football", "calendar", "ics", "scraping", "puppeteer"],
  "author": "PikPakPik",
  "license": "MIT",
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "@types/puppeteer": "^7.0.4",
    "cheerio": "^1.1.2",
    "ics": "^3.8.1",
    "moment": "^2.30.1",
    "puppeteer": "^24.15.0"
  },
  "devDependencies": {
    "@types/cheerio": "^1.0.0",
    "@types/node": "^24.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3"
  }
}
